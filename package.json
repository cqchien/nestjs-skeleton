{
  "name": "nestjs-skeleton",
  "version": "1.0.0",
  "description": "The skeleton for nestjs that follow clean architecture",
  "main": "dist/main.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build:prod": "nest build -p tsconfig.build.json",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main.js",
    "watch:dev": "nodemon --config nodemon.json",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint --fix . --ext .ts",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js",
    "migration:generate": "yarn run typeorm migration:generate -d ormconfig.ts",
    "migration:create": "yarn run typeorm migration:create",
    "migration:revert": "yarn run typeorm migration:revert -d ormconfig.ts",
    "migration:run": "yarn run typeorm migration:run -d ormconfig.ts",
    "schema:drop": "yarn run typeorm schema:drop",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:cov": "NODE_ENV=test jest --coverage",
    "test:debug": "NODE_ENV=test node --inspect-brk -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "NODE_ENV=test jest --config ./test/jest-e2e.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cqchien/nestjs-skeleton.git"
  },
  "author": "cqchien",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cqchien/nestjs-skeleton/issues"
  },
  "homepage": "https://github.com/cqchien/nestjs-skeleton#readme",
  "dependencies": {
    "@nestjs/common": "^11.0.12",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.12",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.12",
    "@nestjs/swagger": "^11.1.0",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/typeorm": "^11.0.0",
    "bcrypt": "^5.1.1",
    "cache-manager-redis-store": "^3.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.7.5",
    "express": "^4.21.2",
    "global": "^4.4.0",
    "helmet": "^8.0.0",
    "lodash": "^4.17.21",
    "mime-types": "^3.0.1",
    "morgan": "^1.10.0",
    "nestjs-cls": "^5.4.2",
    "nodemon": "^3.1.9",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.13.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "source-map-support": "^0.5.21",
    "tslib": "^2.8.1",
    "typeorm": "^0.3.20",
    "yarn": "^1.22.22"
  },
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@nestjs/cli": "^11.0.5",
    "@nestjs/testing": "^11.0.12",
    "@types/bcrypt": "^5.0.2",
    "@types/cache-manager-redis-store": "^3.0.0",
    "@types/compression": "^1.7.5",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.14",
    "@types/mime-types": "^2.1.4",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.10.5",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "cross-env": "^7.0.3",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-import-resolver-typescript": "^4.3.1",
    "eslint-plugin-canonical": "^5.1.3",
    "eslint-plugin-deprecation": "^3.0.0",
    "eslint-plugin-import-helpers": "^2.0.1",
    "eslint-plugin-n": "^17.15.1",
    "eslint-plugin-no-secrets": "^2.1.1",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sonarjs": "^3.0.1",
    "eslint-plugin-unicorn": "^58.0.0",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "lint-staged": "^15.3.0",
    "prettier": "^3.4.2",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.28.0"
  }
}
